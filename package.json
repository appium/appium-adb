{
  "name": "appium-adb",
  "version": "7.7.0",
  "description": "Android Debug Bridge interface",
  "main": "./build/index.js",
  "scripts": {
    "clean": "rm -rf node_modules && rm -f package-lock.json && npm install",
    "prepare": "gulp prepublish",
    "test": "gulp once",
    "watch": "gulp watch",
    "build": "gulp transpile",
    "mocha": "mocha",
    "e2e-test": "gulp transpile && npm run mocha -- -t 600000 --recursive build/test/functional/",
    "coverage": "gulp coveralls",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "precommit-test": "REPORTER=dot gulp once",
    "lint": "gulp lint",
    "lint:fix": "gulp eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appium/appium-adb.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/appium/appium-adb/issues"
  },
  "engines": [
    "node"
  ],
  "bin": {},
  "directories": {
    "lib": "lib"
  },
  "files": [
    "index.js",
    "jars",
    "keys",
    "lib",
    "build/index.js",
    "build/lib"
  ],
  "homepage": "https://github.com/appium/appium-adb",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "appium-support": "^2.24.0",
    "async-lock": "^1.0.0",
    "asyncbox": "^2.3.1",
    "bluebird": "^3.4.7",
    "lodash": "^4.0.0",
    "lru-cache": "^5.0.0",
    "semver": "^6.0.0",
    "shell-quote": "^1.6.1",
    "source-map-support": "^0.5.5",
    "teen_process": "^1.11.0",
    "xmldom": "^0.1.27",
    "xpath": "^0.0.27"
  },
  "pre-commit": [
    "precommit-msg",
    "precommit-test"
  ],
  "devDependencies": {
    "ajv": "^6.5.3",
    "appium-gulp-plugins": "^3.1.0",
    "appium-test-support": "^1.0.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "eslint-config-appium": "^4.0.1",
    "gulp": "^4.0.0",
    "mocha": "^6.0.0",
    "mocha-junit-reporter": "^1.21.0",
    "mocha-multi-reporters": "^1.1.7",
    "pre-commit": "^1.1.3",
    "sinon": "^7.2.3",
    "temp": "^0.9.0"
  },
  "greenkeeper": {
    "ignore": []
  }
}
